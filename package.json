{
  "name": "op-bruno",
  "version": "0.0.5",
  "description": "CLI tool to sync Bruno secrets with 1Password",
  "author": "Kai Nguyen",
  "homepage": "https://github.com/kai-nguyen-aligent/op-bruno",
  "bugs": "https://github.com/kai-nguyen-aligent/op-bruno/issues",
  "repository": "kai-nguyen-aligent/op-bruno",
  "type": "module",
  "main": "dist/index.js",
  "bin": "./bin/run.js",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && shx cp -r src/templates dist/templates",
    "dev": "pnpm run build && pnpm run prepack && ./bin/dev.js",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "prepare": "pnpm run build",
    "lint": "eslint . --ext .ts",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "dependencies": {
    "@1password/op-js": "^0.1.13",
    "@oclif/core": "^4.5.2",
    "@oclif/plugin-help": "^6.2.32",
    "@usebruno/lang": "^0.29.0",
    "chalk": "^5.6.2",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.1"
  },
  "devDependencies": {
    "@aligent/ts-code-standards": "^4.0.2",
    "@types/ejs": "^3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "oclif": "^4.22.0",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "op-bruno",
    "dirname": "op-bruno",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.17.1",
  "license": "MIT"
}
